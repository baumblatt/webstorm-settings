<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="ctrl quote" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt quote" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl semicolon" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
  <action id="FileChooser.TogglePathShowing" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="Images.Editor.ToggleGrid">
    <keyboard-shortcut first-keystroke="alt 1" />
  </action>
  <action id="ParameterInfo" />
</keymap>